<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_122"
    android:layout_height="@dimen/dp_120">


    <ImageView
        android:id="@+id/item_mine_column_img"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_25"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_35" />

    <TextView
        android:id="@+id/item_mine_column_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_mine_column_img"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_12"
        android:text="@string/mine_column_item"
        android:textColor="@color/common_text_color_1"
        android:textSize="@dimen/sp_12" />

</RelativeLayout>